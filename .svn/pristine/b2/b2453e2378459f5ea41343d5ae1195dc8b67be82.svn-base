.tabladinamica-tabla {
	width: 100%;
}

.tabladinamica-caption {
	width: 90%;
	float: left;
	text-align: left;
	font-weight: bold;
	padding-left: 5px;
}

.tabladinamica-addbutton {
	width: 25px;
	float: right;
	padding-right: 1px;
	background-color: white;
}

.tabladinamica-tabla td {
	font-family: 'calibri', sans-serif;
}

.tabladinamica-header>tr>th, .tabladinamica-header>th {
	font: bold 16px Calibri, sans-serif;
	text-align: center;
	background-color: #EF942F;
	color: #FFFFFF;
	padding: 3px 4px;
}
.tabladinamica-header>tr>th:nth-child(1) {
	border-top-left-radius: 5px;
	border-bottom-left-radius: 5px;
}
.tabladinamica-header>tr>th:nth-last-child(1) {
	border-top-right-radius: 5px;
	border-bottom-right-radius: 5px;
}

/* Custom Scroll */
.tabladinamica-container.scrollbar {
	height: 100%;
	overflow-y: scroll;
}
.tabladinamica-container.scrollbar::-webkit-scrollbar {
	width: 9px;
	height: 9px;
}
.tabladinamica-container.scrollbar::-webkit-scrollbar-thumb {
	background-color: #E67B19;
}
