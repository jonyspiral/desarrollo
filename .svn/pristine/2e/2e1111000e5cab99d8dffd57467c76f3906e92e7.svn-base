.divResultados {
	display: none;
	border: 1px solid #D59B2C;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	_margin: 0;
	padding: 0;
	float: left;
	clear: left;
	z-index: 10001;
}

.divResultados div {
	font-family: Calibri, sans-serif;
	font-size: 15px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	text-align: left;
	padding-left: 5px;
}

.divResultados .selected {
	background: #E67B19 50% 50% repeat-x;
	color: #FFFFFF;
	font-weight: bold;
}

.divResultados .unselected {
	background: #FFFFFF;
	color: #000000;
}

.divResultados:hover {
	cursor: pointer;
}